<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/llItemQrResultRoot"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginHorizontal="@dimen/_8sdp"
    android:layout_marginTop="@dimen/_8sdp"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/ivQrResultLogo"
        android:layout_width="@dimen/_24sdp"
        android:layout_height="@dimen/_24sdp"
        tools:background="@drawable/link_icon"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tvQrResultType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_6sdp"
        android:fontFamily="@font/gilroy_bold"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ivQrResultLogo"
        tools:text="URL" />

    <TextView
        android:id="@+id/tvQrResultDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_6sdp"
        android:fontFamily="@font/gilroy_semibold"
        android:textColor="@color/dark_gray"
        app:layout_constraintTop_toBottomOf="@+id/tvQrResultType"
        app:layout_constraintStart_toEndOf="@+id/ivQrResultLogo"
        tools:text="17 Haz 2023 23.49" />

    <TextView
        android:id="@+id/tvQrResultData"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_6sdp"
        android:layout_marginEnd="@dimen/_6sdp"
        android:fontFamily="@font/gilroy_semibold"
        android:textColor="@color/dark_gray"
        app:layout_constraintEnd_toStartOf="@+id/ivAddFavorite"
        app:layout_constraintTop_toBottomOf="@+id/tvQrResultDate"
        app:layout_constraintStart_toEndOf="@+id/ivQrResultLogo"
        tools:text="17 Haz 2023 23.49asdasdasdasd17 Haz 2023 23.49asdasdasdasda17 Haz 2023 23.49asdasdasdasda17 Haz 2023 23.49asdasdasdasda17 Haz 2023 23.49asdasdasdasdaa" />

    <ImageView
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:id="@+id/ivAddFavorite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_8sdp"
        android:background="@drawable/star_empty_icon" />

</androidx.constraintlayout.widget.ConstraintLayout>